{
  "name": "@paranatural/threshold-lint-cli",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "develop": "rollup --watch -c rollup.config.js",
    "format": "prettier --ignore-unknown --write src",
    "lint": "exit 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "build": "rollup -c rollup.config.js",
    "clean": "rimraf dist"
  },
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "threshold-lint": "dist/bin.js"
  },
  "peerDependencies": {},
  "dependencies": {
    "@paranatural/threshold-core": "^0.0.0",
    "@paranatural/threshold-lint": "^0.0.0",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/minimist": "1.2.5",
    "@types/node": "^20.14.2",
    "prettier": "^3.3.1",
    "read-pkg-up": "^11.0.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
