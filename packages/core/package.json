    "read-pkg-up": "^11.0.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
