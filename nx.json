{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "develop": {
      "dependsOn": [
        "^develop"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "preview": {
      "dependsOn": [
        "^preview"
      ]
    },
    "format": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  },
  "defaultBase": "master",
  "useDaemonProcess": false
}
